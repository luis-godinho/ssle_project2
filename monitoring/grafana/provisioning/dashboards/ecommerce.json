{
  "id": null,
  "uid": "ecommerce-main",
  "title": "Ecommerce - Overview",
  "tags": ["ecommerce", "microservices"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "timeseries",
      "title": "API Gateway Requests per Second",
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "increase(api_gateway_requests_total[5m])",
          "legendFormat": "gateway",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Average API Gateway Response Time (s)",
      "gridPos": { "x": 12, "y": 0, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "api_gateway_response_time_seconds",
          "legendFormat": "avg",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Orders Created per Minute",
      "gridPos": { "x": 0, "y": 8, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "rate(order_total_created_total[5m])",
          "legendFormat": "orders created",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Order Value per Minute",
      "gridPos": { "x": 12, "y": 8, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "rate(order_total_value_total[5m])",
          "legendFormat": "â‚¬ / min",
          "refId": "A"
        }
      ]
    },
    {
      "type": "bargauge",
      "title": "Orders by Status",
      "gridPos": { "x": 0, "y": 16, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "order_status_count",
          "legendFormat": "{{status}}",
          "refId": "A"
        }
      ]
    },
    {
      "type": "timeseries",
      "title": "Order Service Request Duration (p95)",
      "gridPos": { "x": 12, "y": 16, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(order_service_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "p95",
          "refId": "A"
        }
      ]
    }
  ],
  "time": {
    "from": "now-15m",
    "to": "now"
  }
}
